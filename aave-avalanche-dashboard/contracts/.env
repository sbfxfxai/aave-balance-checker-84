# Private key for deployment (WARNING: Never commit this file with real private keys)
PRIVATE_KEY=your_private_key_here

# Avalanche RPC URLs
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
AVALANCHE_FUJI_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc

# Snowtrace API key for contract verification
SNOWTRACE_API_KEY=your_snowtrace_api_key_here

# Gas reporting
REPORT_GAS=false

# Etherscan API keys for other networks (if needed)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# Infura project ID (alternative to direct RPC URLs)
INFURA_PROJECT_ID=your_infura_project_id_here

# Alchemy API key (alternative to direct RPC URLs)
ALCHEMY_API_KEY=your_alchemy_api_key_here
